---
title: Sound Calligraph
---

<% content_for :scripts do %>

    <% javascript_include_tag "partials/fonts" %>

<% end %>



<%= partial("partials/dash_display") if locals[:config][:dashboard] %>


<%= javascript_include_tag "partials/CalligraphBase_#{locals[:base]}" %>
<%= javascript_include_tag "partials/calligraphs/#{locals[:calligraph]}" %>

<script>
    $(document).ready(function(){
        window.__c = new Calligraph( <%= locals[:config].to_json %> );
        if(typeof(try_startup) !== 'undefined'){
            try_startup();
        }else{
            window.__c.init();
        }
    })
</script>
